<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="10dp"/>
			<gradient
				android:angle="270"
				android:centerColor="@color/progress_background_c_color"
				android:centerY="0.75"
				android:endColor="#9E9E9E"
				android:startColor="#BDBDBD"/>
		</shape>
	</item>
	<item
		android:id="@android:id/progress">
		<clip>
			<shape>
				<corners
					android:radius="10dp"/>
				<gradient
					android:angle="270"
					android:centerColor="#01579B"
					android:centerY="0.75"
					android:endColor="#0288D1"
					android:startColor="#81D4FA"/>
			</shape>
		</clip>
	</item>

</layer-list>