<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="10dp"/>
			<gradient
				android:angle="270"
				android:centerColor="@color/progress_background_c_color"
				android:centerY="0.75"
				android:endColor="@color/progress_background_e_color"
				android:startColor="@color/progress_background_s_color"/>
		</shape>
	</item>
	<item
		android:id="@android:id/progress">
		<clip>
			<shape>
				<corners
					android:radius="10dp"/>
				<gradient
					android:angle="270"
					android:centerColor="@color/progress_c_color"
					android:centerY="0.75"
					android:endColor="@color/progress_e_color"
					android:startColor="@color/progress_s_color"/>
			</shape>
		</clip>
	</item>

</layer-list>